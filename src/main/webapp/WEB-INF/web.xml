<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<display-name>Archetype Created Web Application</display-name>

	<jsp-config>
		<taglib>
			<taglib-uri>/escape</taglib-uri>
			<taglib-location>/WEB-INF/escape-tags.tld</taglib-location>
		</taglib>
	</jsp-config>

	<servlet>
		<servlet-name>Servlet</servlet-name>
		<servlet-class>com.servlets.Servlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Servlet2</servlet-name>
		<servlet-class>com.servlets.Servlet2</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ServletAnnuaire</servlet-name>
		<servlet-class>com.servlets.ServletAnnuaire</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ServletRestaurant</servlet-name>
		<servlet-class>com.servlets.ServletRestaurant</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ServletModifier</servlet-name>
		<servlet-class>com.servlets.ServletModifier</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ServletAjouter</servlet-name>
		<servlet-class>com.servlets.ServletAjouter</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ServletModifierPlat</servlet-name>
		<servlet-class>com.servlets.ServletModifierPlat</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ServletAnnuaire</servlet-name>
		<url-pattern>/annuaire</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServletRestaurant</servlet-name>
		<url-pattern>/restaurant</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServletModifier</servlet-name>
		<url-pattern>/modifier</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServletAjouter</servlet-name>
		<url-pattern>/ajouter</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServletModifierPlat</servlet-name>
		<url-pattern>/modifierplat</url-pattern>
	</servlet-mapping>

</web-app>
